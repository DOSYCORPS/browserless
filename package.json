{
  "name": "browserless",
  "description": "Chrome Headless API made easy",
  "homepage": "https://browserless.js.org",
  "version": "4.2.1",
  "main": "src/index.js",
  "author": {
    "email": "josefrancisco.verdu@gmail.com",
    "name": "Kiko Beats",
    "url": "https://kikobeats.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kikobeats/browserless.git"
  },
  "bugs": {
    "url": "https://github.com/Kikobeats/browserless/issues"
  },
  "keywords": [
    "browser",
    "browserless",
    "chrome",
    "chromeless",
    "headless",
    "html",
    "pdf",
    "puppeteer",
    "screenshot",
    "text"
  ],
  "dependencies": {
    "debug": "~4.1.0",
    "extract-domain": "~2.0.4",
    "generic-pool": "~3.4.2",
    "is-tracking-domain": "~1.1.6",
    "tempy": "~0.2.1"
  },
  "devDependencies": {
    "asciichart": "latest",
    "browser-sync": "latest",
    "concurrently": "latest",
    "coveralls": "latest",
    "fmt-obj": "latest",
    "git-dirty": "latest",
    "gulp": "latest",
    "gulp-autoprefixer": "latest",
    "gulp-concat": "latest",
    "gulp-cssnano": "latest",
    "gulp-strip-css-comments": "latest",
    "gulp-uglify": "latest",
    "husky": "latest",
    "lint-staged": "latest",
    "lodash": "latest",
    "looks-same": "latest",
    "measured": "latest",
    "meow": "latest",
    "mocha": "latest",
    "nyc": "latest",
    "p-all": "latest",
    "p-event": "latest",
    "percentile": "latest",
    "prettier-standard": "latest",
    "pretty-ms": "latest",
    "process-stats": "latest",
    "puppeteer": "latest",
    "should": "latest",
    "standard": "11",
    "standard-markdown": "latest",
    "term-img": "latest",
    "time-span": "latest"
  },
  "engines": {
    "node": ">= 6"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "clean": "rm -rf node_modules",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "dev:docs": "concurrently \"gulp\" \"npm run server\"",
    "lint": "standard-markdown README.md && standard",
    "precommit": "lint-staged",
    "pretest": "npm run lint",
    "pretty": "prettier-standard index.js test/**/*.js --single-quote",
    "server": "browser-sync start --server --files \"index.html, README.md, static/**/*.(css|js)\"",
    "test": "nyc mocha"
  },
  "license": "MIT",
  "lint-staged": {
    "*.js": [
      "prettier-standard",
      "git add"
    ]
  },
  "peerDependencies": {
    "puppeteer": "1.x"
  },
  "standard": {
    "env": [
      "mocha"
    ]
  }
}
